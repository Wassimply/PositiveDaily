{
	//Should always be 2, version 1 is unsupported
	"manifest_version" : 2,
	"name" : "Positive Daily",
	"version" : "1",
	"content_scripts" : [
		{
			//to inject into some pages, use match patterns
			//example: ["https://mail.google.com/*", "http://mail.google.com/*"]s
			"matches" : ["<all_urls>"],
			"js" : ["jquery-2.1.3.min.js", "PostiveDaily.js"]
		}
		"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },

	]
}
